{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
<div class="hero-container">
    <h1>Inscription</h1>


    {{ form_start(form) }}
    {{ form_row(form.nom) }}
    {{ form_row(form.email) }}
    {{ form_row(form.plainPassword) }}
    <button type="submit" class="btn btn-primary">S'inscrire</button>
    {{ form_end(form) }}
</div>
{% endblock %}
